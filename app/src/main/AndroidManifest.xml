<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="frc.team449.deepspacescoutingapp">

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name="frc.team449.deepspacescoutingapp.activities.Prematch">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="frc.team449.deepspacescoutingapp.activities.Sandstorm" />
        <activity android:name="frc.team449.deepspacescoutingapp.activities.Teleop" />
        <activity android:name="frc.team449.deepspacescoutingapp.activities.Submitted" />
        <activity android:name="frc.team449.deepspacescoutingapp.activities.Endgame" />
        <activity android:name="frc.team449.deepspacescoutingapp.activities.General" />
    </application>

</manifest>