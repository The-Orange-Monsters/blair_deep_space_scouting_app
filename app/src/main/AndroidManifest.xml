<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="frc.team449.scoutingappframe">

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Prematch" />
        <activity android:name=".Activities.Auto" />
        <activity android:name=".Activities.Teleop" />
        <activity android:name=".FixErrors" />
        <activity android:name=".Submit" />
        <activity android:name=".Activities.Submitted" />
        <activity android:name=".Activities.Endgame" />
    </application>

</manifest>