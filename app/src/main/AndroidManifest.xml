<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="frc.team449.scoutingapptemplate">

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Prematch" />
        <activity android:name=".Auto" />
        <activity android:name=".Teleop" />
        <activity android:name=".Endgame" />
        <activity android:name=".FixErrors" />
        <activity android:name=".Submit" />
        <activity android:name=".Submitted" />

    </application>
</manifest>
